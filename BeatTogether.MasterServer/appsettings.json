{
  "Urls": "http://0.0.0.0:8989",
  "RabbitMQ": {
    "HostName": "127.0.0.1"
  },
  "Serilog": {
    "File": {
      "Path": "logs/BeatTogether.MasterServer-{Date}.log"
    },
    "MinimumLevel": {
      "Default": "Information",
      "Overrides": {
        "Microsoft": "Warning"
      }
    }
  },
  "ServerConfiguration": {
    "VersionRanges": [
      {
        "MinVersion": "1.37.0",
        "MaxVersion": "1.39.1"
      },
      {
        "MinVersion": "1.40.0"
      }
    ],
    "AuthenticateClients": true,
    "AuthedClients": [
      "Steam",
      "Oculus",
      "Pico",
      "OculusQuest"
    ]
  }
}
